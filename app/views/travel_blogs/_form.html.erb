<%= simple_form_for(@travel_blog) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :paragraph1 %>
    <%= f.input :paragraph2 %>
    <%= f.input :paragraph3 %>
    <%= f.input :paragraph4 %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
