<%= link_to 'New Travel blog', new_travel_blog_path %>

<div id="owl-demo" class="owl-carousel owl-theme">
  <% @travel_blogs.each do |travel_blog| %>
    <div class="item">
      <%= link_to image_tag(travel_blog.image.url(:medium)), travel_blog %>
      <h3>Travel </h3>
      <h2><%= travel_blog.title %></h2>
    </div>
  <% end %>
</div>

<div class="travel_blogs" id="transitions-enabled">
    <% @travel_blogs.each do |travel_blog| %>
      <div class="box panel panel-default">
        <td><%= link_to image_tag(travel_blog.image.url(:medium)), travel_blog %></td>
        <h3>Travel </h3>
        <h2><%= travel_blog.title %></h2>
      </div>
    <% end %>
</div>







<div class="center">
  <%= will_paginate @posts, renderer: BootstrapPagination::Rails %>
</div>